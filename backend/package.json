{
  "name": "the-pet-hotel-backend",
  "version": "1.0.0",
  "description": "Backend",
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon ./server.js",
    "watch": "nodemon",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "logs": "firebase functions:log",
    "start:development": "cross-env NODE_ENV=development nodemon ./server.js",
    "start:production": "cross-env NODE_ENV=production nodemon ./server.js",
    "test": "nodemon --exec 'cross-env NODE_ENV=test mocha ./src/**/*.test.js --exit || exit 1'",
    "deploy:appengine:development": "gcloud app deploy app-engine.development.yaml --project the-pet-hotel",
    "predeploy:production": "firebase use production && firebase functions:config:set env.value=\"production\"",
    "deploy:production": "firebase deploy --only functions",
    "deploy:appengine:production": "gcloud app deploy app-engine.production.yaml --project the-pet-hotel",
    "mongod": "run-rs -v 4.0.0 --shell"
  },
  "dependencies": {
    "apollo-server-express": "^2.18.2",
    "bcrypt": "^3.0.6",
    "cors": "2.8.5",
    "express": "^4.17.1",
    "express-graphql": "0.9.0",
    "formidable": "1.2.1",
    "graphql": "14.5.8",
    "graphql-fields": "2.0.3",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "4.0.6",
    "graphql-type-json": "0.3.0",
    "helmet": "3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mongoose": "^5.7.8",
    "nodemailer": "6.3.1"
  },
  "engines": {
    "node": "10"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "^6.0.3",
    "mocha": "6.2.2",
    "node-mocks-http": "1.8.0",
    "nodemon": "^1.19.4"
  }
}
